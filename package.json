    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "echarts-gl": "^2.0.9",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "papaparse": "^5.4.1",
